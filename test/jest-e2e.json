{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "globalSetup": "<rootDir>/test/setup.ts",
  "globalTeardown": "<rootDir>/test/teardown.ts",
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^data/(.*)$": "<rootDir>/data/$1",
    "^prisma/(.*)$": "<rootDir>/prisma/$1"
  }
}